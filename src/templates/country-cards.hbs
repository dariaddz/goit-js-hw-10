

{{#each this}}
<div class="country-title">
    <img class="country-flag" src="{{flags.svg}}" alt="{{name}}" width="50">
    <p class="country-name"> {{name.official}}</p>
</div>
<ul class="country-detailed-info">
    <li class="country-capital">{{capital}}</li>
    <li class="country-population">{{population}}</li>
    {{!-- <li class="country-languages">{{#each languages}}
    {{name}}
    {{/each}}</li> --}}
</ul>
{{/each}}